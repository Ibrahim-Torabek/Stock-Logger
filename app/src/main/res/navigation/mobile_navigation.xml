<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_main">
    <fragment
        android:id="@+id/nav_main"
        android:name="ibrahim.example.stocklogger.fragments.MainFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_nav_main_to_addStockFragment"
            app:destination="@id/addStockFragment" />
        <action
            android:id="@+id/action_nav_main_to_sellStockFragment2"
            app:destination="@id/sellStockFragment" />
    </fragment>
    <fragment
        android:id="@+id/addStockFragment"
        android:name="ibrahim.example.stocklogger.fragments.AddStockFragment"
        android:label="Add Stock"
        tools:layout="@layout/fragment_add_stock" />
    <fragment
        android:id="@+id/sellStockFragment"
        android:name="ibrahim.example.stocklogger.fragments.SellStockFragment"
        android:label="Sell Stock"
        tools:layout="@layout/fragment_sell_stock" />
    <fragment
        android:id="@+id/nav_sold"
        android:name="ibrahim.example.stocklogger.fragments.SoldStockFragment"
        android:label="Sold Stocks"
        tools:layout="@layout/fragment_sold_stock" />
    <fragment
        android:id="@+id/nav_about"
        android:name="ibrahim.example.stocklogger.fragments.AboutFragment"
        android:label="About Us"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/nav_contact"
        android:name="ibrahim.example.stocklogger.fragments.ContactFragment"
        android:label="Contact"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/contactCallFragment"
        android:name="ibrahim.example.stocklogger.fragments.contact.ContactCallFragment"
        android:label="fragment_contact_call"
        tools:layout="@layout/fragment_contact_call" />
    <fragment
        android:id="@+id/contactMessageFragment"
        android:name="ibrahim.example.stocklogger.fragments.contact.ContactMessageFragment"
        android:label="fragment_contact_message"
        tools:layout="@layout/fragment_contact_message" />
    <fragment
        android:id="@+id/contactEmailFragment"
        android:name="ibrahim.example.stocklogger.fragments.contact.ContactEmailFragment"
        android:label="fragment_contact_email"
        tools:layout="@layout/fragment_contact_email" />
    <fragment
        android:id="@+id/nav_credits"
        android:name="ibrahim.example.stocklogger.fragments.CreditsFragment"
        android:label="Credits"
        tools:layout="@layout/fragment_credits" />
</navigation>